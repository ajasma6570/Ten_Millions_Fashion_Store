<body>
    <div >
        <header class="header " style="background-color: rgba(62, 177, 150, 1)">
            <div class="header-middle ">
                <div class="container-fluid">
                    <div class="header-left">
                        <button class="mobile-menu-toggler">
                            <span class="sr-only">Toggle mobile menu</span>
                            <i class="icon-bars"></i>
                        </button>

                        <a href="/" class="logo">
                            <img src="/assets/images/figma/text-1684992384765.png" alt="Molla Logo" width="140" height="25">
                        </a>

                        <nav class="main-nav">
                            <ul class="menu ">
                                <li class="megamenu-container ">
                                    <a href="/" class="">Home</a>
                                </li>
                                <li>
                                    <a href="/shirts" class="">Shirts</a>
                                </li>
                                <li>
                                    <a href="/about" class="">About</a>
                                </li>
                                <li>
                                    <a href="/contactus" class="">Contact us</a>
                                </li>
                            </ul>
                        </nav><!-- End .main-nav -->
                    </div><!-- End .header-left -->

                    <div class="header-right">
                        <div class="header-search  header-search-visible">
                            <a href="#" class="search-toggle" role="button"><i class="icon-search"></i></a>
                            <form action="/productsearch" method="post">
                                <div class="header-search-wrapper">
                                    <label for="q" class="sr-only">Search</label>
                                    <input type="search" class="form-control" name="pname" id="q" placeholder="Search product ..." required>
                                    <button class="btn btn-primary" type="submit"><i class="icon-search"></i></button>
                                </div><!-- End .header-search-wrapper -->
                            </form>
                        </div><!-- End .header-search -->
                        <div class="dropdown cart-dropdown ">
                            
							<a href="#" class="dropdown-toggle" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" data-display="static">

                                <i class="icon-user"></i>
                                <% if (locals.session) { %>
                                    <span style="font-size: 2rem;"><%-locals.title-%></span>
                                    <% } else { %>
                                        <span><%-locals.title-%></span>
                                        
                                        <% } %>

                                
	                        </a>
							<div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                <ul>
                                    <% if (locals.session) { %>
                                        <li><a class="list-group-item <%-locals.log-%>" href="/userprofile">Profile</a></li>
                                        <li><a class="list-group-item <%-locals.log-%>" href="/logout">Logout</a></li>
                                        <% } else { %>
                                            
                                            <li><a class="list-group-item <%-locals.sign1-%>" href="/login">Signin </a></li>
                                            <li><a class="list-group-item  <%-locals.sign2-%>" href="/signup">Sign up</a></li>
                                            
                                            <% } %>
                                  
                                    
                                </ul>
							  </div>

	                    </div>
                        
                        <a href="#" class="wishlist-link">
